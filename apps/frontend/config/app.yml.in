      0:
      1: skitouring
      2: snow_ice_mixed
      3: mountain_climbing
      4: rock_climbing
      5: ice_climbing
      6: hiking
    
  forum:
    bdd_prefix: punbb_
    cookie_language_name: language 
    config_path: %SF_ROOT_DIR%/web/forums/config.php
  
  captcha:
    alphabet: abcdefghjkmnptwxyz234678
    length:   6
    
  # in days
  pending_users_lifetime: 7
    
  outgoing_emails:
      sender: @SENDER@
      from: @FROM@
      reply_to: @REPLY_TO@

  # parameter for the remember cookie
  remember_key:
    expiration_age: 2678400 # = 31 days
    cookie_name:    c2c_remember
    random:         899cfcae74ca2beb07b3c8420ad9667b
    
  gmaps_key: @GMAPS_KEY@
  
  ajax_feedback:
    div_name_success:  ajax_feedback_success
    div_name_failure:  ajax_feedback_failure
    div_style_inline:  ajax_inline_feedback
    div_style_absolute: ajax_feedback
  
  autocomplete:
    min_chars: 3
    max_results: 15
    
  shortdescription_max_length: 30
  comment_max_lengh: 200
  
  # information inserted in db by historymetadata
  user_creation:
    master_id:  2
    is_minor:   false
    comment:    User creation
  
  list:
    maxline_number: 30
    default_order:  asc
  
  form:
    input_group_class:  form-row
    label_class:        fieldname
    global_error_class: global_form_error
    field_error:        field_error
    
  personalization:
    filter_timeout:           2678400 # = 31 days
    cookie_languages_name:    filtered_languages
    cookie_activities_name:   filtered_activities
    cookie_places_name:       filtered_places
    cookie_switch_name:       filter_switch
      
  gpx:
    # filter based on distance : 
    # its value is the minimum distance between two successive points after filtering.
    filter_distance_switch:   true
    filter_distance_value:    5
    # hdop is horizontal dilution of precision in GPX files
    # this filter is useful to remove unprecise points:
    filter_hdop_switch:       true
    filter_hdop_value:        4    

  traces:
    maxfilesize: 512000       # 512 ko maybe more ?
  
  upload_dir: uploads
  
  images:
    directory_name:      images
    temp_directory_name: images_temp
    validation:
      weight:          2097152                                 #in o | = 2mo
      mime_types:      ['image/jpg', 'image/jpeg', 'image/pjpeg', 'image/gif', 'image/png']
      file_extensions: ['jpg', 'jpeg', 'gif', 'png']
      max_size:            { height: 2000, width: 2000 }           #in px
      min_size:            { height: 300, width: 300 }
    types:             
      small:           
        suffix:        SI
        dimensions:    { height: 120, width: 120 }            #in px
        square:        true
        inflate:       true
      medium:
        suffix:        MI
        dimensions:    { height: 250, width: 250 }            #in px
        square:        false
        inflate:       true
      big:                                                     
        suffix:        BI                                      
        dimensions:    { height: 600, width: 700 }            #in px
        square:        false
        inflate:       false
        
  # this section is used to set group names that are stored in bdd
  # easy management :D
  group_name:
    pending:  pending
    logged:   logged
    
  # define default name (to use in symfony)  and value (thats what is in db)
  credentials:
     admin:        admin
     mgmt_contrib: contributor_management
     mgmt_member:  member_management
     moderator:    moderator
  
